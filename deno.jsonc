{
  "tasks": {
    "scaffold": "deno run -A --env scaffold.ts",
    "readme": "deno run -A --env buildReadme.ts"
  },
  "permissions": {
    "default": {
      "read": true
    }
  },
  "imports": {
    "/u/": "/algo-utils/"
  },
  "fmt": {
    "useTabs": true,
    "lineWidth": 80,
    "indentWidth": 4,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  }
}
